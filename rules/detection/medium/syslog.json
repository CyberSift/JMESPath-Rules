[
  {
    "title": "Potential Data Leak - Dropbox Usage",
    "expression": "\"ASN Description\"=='DROPBOX'",
    "severity": "medium",
    "type": "syslog"
  },
  {
    "title": "SMB to public IP",
    "expression": "DestinationPort=='445' && isSourceInternal=='true' && isDestinationInternal=='false' && denied_flow==`false` && DestinationAddress!='0.0.0.0'",
    "severity": "medium",
    "type": "syslog"
  },
  {
    "title": "Connection From Known Threat (Source IP Known Threat)",
    "expression": "contains(Abnormal_Notes || '','Known_Threat_Inbound') && denied_flow==`false` && \"cybersift.deduped\"=='false'",
    "severity": "medium",
    "type": "syslog"
  },
  {
    "title": "Connection To Known Threat (Destination IP Known Threat)",
    "expression": "contains(Abnormal_Notes || '','Known_Threat_Outbound') && denied_flow==`false` && \"cybersift.deduped\"=='false' && !(contains(to_string(\"cybersift.threat_info\".isp), 'Cloudflare') || contains(to_string(\"cybersift.threat_info\".isp), 'CLOUDFLARE') || contains(to_string(\"cybersift.threat_info\".isp), 'Fastly') || contains(to_string(\"cybersift.threat_info\".isp), 'Amazon') || contains(to_string(\"cybersift.threat_info\".isp), 'AMAZON') || contains(to_string(\"cybersift.threat_info\".isp), 'Microsoft'))",
    "severity": "medium",
    "type": "syslog"
  }
]
